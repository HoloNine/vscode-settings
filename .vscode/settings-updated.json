{
  "editor.autoIndent": "full",
  "editor.cursorBlinking": "solid",
  "editor.cursorStyle": "line",
  "editor.fontSize": 12,
  "editor.fontFamily": "JetBrains Mono",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": false,
  "editor.formatOnSave": true,
  "editor.letterSpacing": 0.6,
  "editor.lineHeight": 24,
  "editor.minimap.enabled": false,
  "editor.parameterHints.enabled": true,
  "editor.renderWhitespace": "none",
  "editor.tabCompletion": "on",
  "editor.wordWrap": "off",
  "editor.scrollbar.horizontalScrollbarSize": 10,
  "editor.scrollbar.verticalScrollbarSize": 10,
  "files.autoSave": "afterDelay",
  "debug.console.fontSize": 12,
  "terminal.integrated.fontSize": 12,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "files.defaultLanguage": "typescript",
  "files.trimTrailingWhitespace": true,
  "editor.suggestSelection": "first",
  "workbench.startupEditor": "readme",
  "task.autoDetect": "off",
  "git.enableSmartCommit": true,
  "git.autofetch": true,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.hideUnchangedRegions.enabled": true,
  "githubPullRequests.createOnPublishBranch": "never",
  "editor.stickyScroll.enabled": false,
  "breadcrumbs.enabled": false,
  "git.confirmSync": false,
  "extensions.ignoreRecommendations": true,
  "notebook.stickyScroll.enabled": true,
  "files.exclude": {
    "**/__pycache__": true,
    "**/.DS_Store": true,
    "**/.git": true,
    "**/*.spec.ts": true,
    "**/package-lock.json": true,
    "**/node_modules": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html-hubl]": {
    "editor.formatOnSave": false,
    "editor.formatOnType": false
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css-hubl]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "files.autoSaveDelay": 9000,
  "workbench.colorCustomizations": {
    "[Default Dark Modern]": {
      "activityBar.activeBorder": "#D9596A",
      "activityBar.border": "#090909",
      "activityBar.foreground": "#FFFFFF",
      "activityBarBadge.background": "#D9596A",
      "activityBarBadge.foreground": "#ffffff",
      "button.background": "#D9596A",
      "button.hoverBackground": "#D9596A",
      "editor.background": "#181818",
      "editorGroupHeader.tabsBorder": "#090909",
      "editorGutter.modifiedBackground": "#D9596A",
      "editorIndentGuide.activeBackground1": "#090909",
      "editorIndentGuide.background1": "#181818",
      "focusBorder": "#09090900",
      "menu.selectionBackground": "#D9596A",
      "notifications.border": "#090909",
      "panel.border": "#090909",
      "panelInput.border": "#090909",
      "panelTitle.activeBorder": "#D9596A",
      "progressBar.background": "#D9596A",
      "sideBar.border": "#090909",
      "sideBarSectionHeader.border": "#090909",
      "statusBar.border": "#090909",
      "statusBar.debuggingBackground": "#D9596A",
      "statusBar.focusBorder": "#D9596A",
      "statusBarItem.focusBorder": "#D9596A",
      "statusBarItem.remoteBackground": "#D9596A",
      "tab.activeBackground": "#181818",
      "tab.activeBorder": "#090909",
      "tab.activeBorderTop": "#D9596A",
      "tab.border": "#090909",
      "tab.hoverBackground": "#181818",
      "tab.unfocusedActiveBorderTop": "#090909",
      "terminal.tab.activeBorder": "#D9596A",
      "textBlockQuote.background": "#090909",
      "textCodeBlock.background": "#090909",
      "titleBar.border": "#090909",
      "welcomePage.progress.foreground": "#D9596A",
      "welcomePage.tileBackground": "#090909",
      "commandCenter.debuggingBackground": "#D9596A42",
      "commentsView.unresolvedIcon": "#D9596A",
      "editorCommentsWidget.rangeActiveBackground": "#D9596A1a",
      "editorCommentsWidget.rangeBackground": "#D9596A1a",
      "editorCommentsWidget.unresolvedBorder": "#D9596A",
      "editorOverviewRuler.modifiedForeground": "#D9596A99",
      "editorStickyScroll.shadow": "#00000000",
      "editorWhitespace.foreground": "#090909",
      "extensionBadge.remoteBackground": "#D9596A",
      "extensionButton.background": "#D9596A",
      "extensionButton.prominentBackground": "#D9596A",
      "inlineChat.shadow": "#00000000",
      "inlineChatInput.focusBorder": "#D9596A",
      "list.activeSelectionBackground": "#090909c6",
      "list.focusOutline": "#D9596A00",
      "list.hoverBackground": "#090909c6",
      "list.inactiveSelectionBackground": "#090909c6",
      "listFilterWidget.shadow": "#00000000",
      "minimapGutter.modifiedBackground": "#D9596A",
      "multiDiffEditor.border": "#090909",
      "notebook.cellInsertionIndicator": "#D9596A",
      "notebook.focusedCellBorder": "#D9596A",
      "notebook.focusedEditorBorder": "#D9596A",
      "panelStickyScroll.shadow": "#00000000",
      "sash.hoverBorder": "#D9596A",
      "scrollbar.shadow": "#00000000",
      "settings.focusedRowBorder": "#D9596A",
      "tree.indentGuidesStroke": "#18181800",
      "editor.foreground": "#d4d4d4",
      "editor.lineHighlightBackground": "#090909c6",
      "editor.lineHighlightBorder": "#09090900"
    }
  },
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "comment",
          "comment.block.documentation.phpdoc keyword.other.phpdoc",
          "entity.name.type.instance.jsdoc"
        ],
        "settings": {
          "foreground": "#ffffff69"
        }
      },
      {
        "scope": ["variable", "variable.other", "variable.other.readwrite"],
        "settings": {
          "foreground": "#d19166"
        }
      },
      {
        "scope": [
          "storage.type.phpdoc.php",
          "support.type.primitive.phpdoc.php",
          "comment.block.documentation.phpdoc.php",
          "storage.type.class.jsdoc",
          "variable.other.jsdoc"
        ],
        "settings": {
          "foreground": "#ffffff51"
        }
      },
      {
        "scope": [
          "keyword",
          "constant.numeric",
          "keyword.operator",
          "variable.other.constant.object.js",
          "support.type.property-name.json"
        ],
        "settings": {
          "foreground": "#d19a66"
        }
      },
      {
        "scope": [
          "entity.name.class",
          "support.class",
          "variable.other.constant"
        ],
        "settings": {
          "foreground": "#e5c07b"
        }
      },
      {
        "scope": [
          "variable.property",
          "variable.other.property",
          "entity.name.function",
          "support.function"
        ],
        "settings": {
          "foreground": "#D9596A"
        }
      },
      {
        "scope": ["string"],
        "settings": {
          "foreground": "#c3c3c3"
        }
      },
      {
        "scope": [
          "support.function.magic",
          "entity.name.type.namespace.php",
          "entity.name.type.class.php",
          "keyword.control.return.php",
          "keyword.control.if.php",
          "keyword.control.foreach.php",
          "storage.type.js",
          "keyword.control.flow.js",
          "keyword.control.conditional.js",
          "entity.other.attribute-name.js"
        ],
        "settings": {
          "foreground": "#867ce6"
        }
      }
    ]
  },
  "workbench.editor.editorActionsLocation": "titleBar",
  "workbench.navigationControl.enabled": false,
  "window.commandCenter": false,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "git.openRepositoryInParentFolders": "always",
  "workbench.editor.showTabs": "none",
  "files.associations": {
    "*.module": "php",
    "*.blade.php": "php",
    "*.css": "css-hubl",
    "*.html": "html-hubl"
  },
  "editor.tabSize": 2,
  "workbench.iconTheme": "charmed-icons",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "emmet.includeLanguages": {
    "html-hubl": "html"
  },
  "workbench.activityBar.location": "hidden",
  "github.copilot.nextEditSuggestions.enabled": true,
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": [
        "${env:windir}\\Sysnative\\cmd.exe",
        "${env:windir}\\System32\\cmd.exe"
      ],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": {
      "source": "Git Bash"
    },
    "Windows PowerShell": {
      "path": "C:\\WINDOWS\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
    }
  }
}
